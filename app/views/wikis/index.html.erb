<h1 align="center">Wikis</h1>
<% if current_user %>
  <div align="center", class="col-md-4">
  <%= link_to '<button type="button">Create New Wiki</button>'.html_safe, new_wiki_path %>
  </div>
<% end %>
<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <p><%= link_to wiki.title, wiki %></p>
      </h4>
    </div>
  </div>
<% end %>
